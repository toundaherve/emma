


		plpgsql					

	statement

	OPEN cursor_name(variables)
	OPEN cursor_name [[NO] SCROLL] FOR query
	CLOSE cursor_name
	UPDATE/DELETE ... WHERE CURRENT OF cursor_name
	EXECUTE query USING variable
